        <div class="btn-group record_actions">
          <a class="btn btn-info" href="{{ path('proprietaire') }}">
            <i class="fa fa-list">
            </i> {{ 'Back to the list'|trans({}, 'admin') }}
          </a>
          <a class="btn btn-primary" href="{{ path('proprietaire_edit', {id: user.id  }) }}">
            <i class="fa fa-edit">
            </i> {{ 'Edit'|trans({}, 'admin') }}
          </a>
          <button class="btn btn-danger" type="submit">
            <i class="fa fa-trash-o">
